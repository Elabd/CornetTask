{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "simplemde@1.11.2",
      "destination": "wwwroot/lib/simplemde/"
    },
    {
      "provider": "cdnjs",
      "library": "moment.js@2.24.0",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@3.1.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@1.10.2",
      "destination": "wwwroot/lib/jquery/dist"
    },
    {
      "library": "jquery-validate@1.14.0",
      "destination": "wwwroot/lib/jquery-validation/dist/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "underscore.js@1.8.1",
      "destination": "wwwroot/lib/underscore.js"

    }

  ]
}